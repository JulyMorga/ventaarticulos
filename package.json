{
  "name": "julian",
  "version": "1.0.0",
  "description": "Mi primer proyecto",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "imagemin": "imagemin imagenes/* --out-dir dist/imagenes",
    "usemin": "usemin paginas/index.html -d dist --htmlmin true -o dist/paginas/index.html && usemin paginas/contacto.html -d dist/paginas --htmlmin true -o dist/paginas/contacto.html && usemin paginas/nosotros.html -d dist --htmlmin true -o dist/paginas/nosotros.html && usemin paginas/precios.html -d dist --htmlmin true -o dist/paginas/precios.html && usemin paginas/productos.html -d dist --htmlmin true -o dist/paginas/productos.html",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "author": "JMorganella",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "cssmin": "^0.4.3",
    "del": "^6.0.0",
    "grunt": "^1.5.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "less": "^3.12.2",
    "lite-server": "^2.5.4",
    "node-less": "^1.0.0",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglify-js": "^3.10.4",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
